# Database Configuration
DB_DRIVER=com.mysql.cj.jdbc.Driver
DB_URL=jdbc:mysql://localhost:3306/checkscam?createDatabaseIfNotExist=true&useUnicode=true&characterEncoding=utf-8&useSSL=false
DB_USERNAME=your_db_username
DB_PASSWORD=your_db_password

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_here
JWT_ACCESS_TOKEN_VALIDITY=86400
JWT_REFRESH_TOKEN_VALIDITY=8640000

# Gemini API Configuration
GEMINI_API_URL=https://generativelanguage.googleapis.com/v1beta/models/gemini-2.0-flash:generateContent
GEMINI_API_KEY=your_gemini_api_key
GEMINI_TIMEOUT=30000
GEMINI_MAX_RETRIES=3

# Google reCAPTCHA Configuration
GOOGLE_RECAPTCHA_SECRET=your_recaptcha_secret
GOOGLE_RECAPTCHA_VERIFY_URL=https://www.google.com/recaptcha/api/siteverify

# OpenRouter API Configuration
OPENROUTER_API_URL=https://openrouter.ai/api/v1/chat/completions
OPENROUTER_API_KEY=your_openrouter_api_key
OPENROUTER_MODEL=google/gemini-2.0-flash-001
OPENROUTER_TIMEOUT=30000
OPENROUTER_MAX_RETRIES=3
OPENROUTER_TEMPERATURE=0.7
OPENROUTER_MAX_TOKENS=1200

# Google OAuth2 Configuration
GOOGLE_OAUTH2_CLIENT_ID=your_google_oauth2_client_id

# Google Safe Browse Configuration
GOOGLE_SAFE_BROWSE_API_KEY=your_google_safe_browse_api_key
GOOGLE_SAFE_BROWSE_API_URL=https://safeBrowse.googleapis.com/v4/threatMatches:find

# Google Web Risk Configuration
GOOGLE_WEB_RISK_API_KEY=your_google_web_risk_api_key
GOOGLE_WEB_RISK_API_URL=https://webrisk.googleapis.com/v1/uris:search

# PhishTank Configuration
PHISHTANK_API_KEY=your_phish_tank_api_key
PHISHTANK_API_URL=http://checkurl.phishtank.com/checkurl/

# VirusTotal Configuration
VIRUSTOTAL_API_KEY=your_virustotal_api_key
VIRUSTOTAL_API_URL=https://www.virustotal.com/api/v3/urls

# URLScan Configuration
URLSCAN_API_KEY=your_urlscan_api_key
URLSCAN_API_URL=https://urlscan.io/api/v1/scan/

# File Upload Configuration
UPLOAD_DIR=uploads
MAX_FILE_SIZE=50MB
MAX_REQUEST_SIZE=50MB

# Server Configuration
SERVER_PORT=8080

# Playwright Configuration
PLAYWRIGHT_TIMEOUT=1000
PLAYWRIGHT_DEFAULT_PROTOCOL=https://
PLAYWRIGHT_VIEWPORT_WIDTH=720
PLAYWRIGHT_VIEWPORT_HEIGHT=1080

# WebSocket CORS Configuration
WEBSOCKET_CORS_ALLOWED_ORIGINS=http://localhost:4200,http://localhost:3000,http://127.0.0.1:4200
